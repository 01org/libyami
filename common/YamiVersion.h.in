#ifndef YAMI_VERSION_H
#define YAMI_VERSION_H

/**
 * yami_major_version
 */
#define YAMI_MAJOR_VERSION    @YAMI_MAJOR_VERSION@

/**
 * yami_minor_version
 */
#define YAMI_MINOR_VERSION    @YAMI_MINOR_VERSION@

/**
 * yami_micro_version
 */
#define YAMI_MICRO_VERSION    @YAMI_MICRO_VERSION@

/**
 * yami_version_string
 */
#define YAMI_VERSION_STRING    "@YAMI_VERSION@"

/**
 * yami_version_int
 */
#define YAMI_VERSION_INT     ((YAMI_MAJOR_VERSION << 24) | \
                                 (YAMI_MINOR_VERSION << 16) | \
                                 (YAMI_MICRO_VERSION << 8))


#endif /* YAMI_VERSION_H */
